#lang racket

;;; Just for fun
(define concentric-turtles
  (lambda (length n)
    (let kernel ([count n]
                 [sides 1])
      (cond
        [(zero? count)
         "many-turtles completed"]
        [else
         (turtle-polygon! test-turtle length sides)
         (kernel (- count 1) (+ sides 1))]))))